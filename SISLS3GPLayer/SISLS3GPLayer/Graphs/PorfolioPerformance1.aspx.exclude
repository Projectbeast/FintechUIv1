                                    <asp:DropDownList ID="ddlAssetType" runat="server" AutoPostBack="true" OnSelectedIndexChanged="ddlAssetType_SelectedIndexChanged" ToolTip="Asset Type"
                                        Width="60%">
                                    </asp:DropDownList>
