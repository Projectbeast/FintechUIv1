                                        &nbsp;<asp:Button runat="server" ID="btnClear" CausesValidation="false" CssClass="styleSubmitButton"
                                        Text="Clear" OnClick="btnClear_Click" OnClientClick="return fnConfirmClear();" />
                                </td>
                            </tr>
                        </table>
                    </asp:Panel>
